{
  "name": "trillo",
  "version": "1.0.0",
  "description": "trillo app",
  "main": "index.js",
  "scripts": {
    "sass": "sass sass/main.scss css/style.css",
    "postcss": "postcss css/style.css --use autoprefixer -o css/style.prefixed.css",
