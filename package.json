    "devserver": "live-server",
    "start": "npm-run-all --parallel devserver sass"
  },
  "author": "Tedo Gulisashvili",
  "license": "ISC",
  "devDependencies": {
    "autoprefixer": "^10.4.20",
    "live-server": "^1.2.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.1.0",
    "sass": "^1.82.0"
  }
}
